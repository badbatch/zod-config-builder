{
  "name": "zcb",
  "description": "Build configs with type safety from zod schema.",
  "version": "0.0.25",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/zod-config-builder",
  "repository": "https://github.com/badbatch/zod-config-builder",
  "bugs": "https://github.com/badbatch/zod-config-builder/issues",
  "type": "module",
  "bin": {
    "zcb": "./bin/zcb.js"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/cjs/index.d.cts",
  "exports": {
    "types": {
      "import": "./dist/types/esm/index.d.ts",
      "require": "./dist/types/cjs/index.d.cts"
    },
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "commit": "commit",
    "compile": "pnpm run /^compile:.*/",
    "compile:cjs": "MODULE_SYSTEM=cjs rollup -c ./rollup.config.cjs",
    "compile:esm": "rollup -c ./rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json && cts-types build dist/types/esm dist/types/cjs",
    "cut:changelog": "changelog",
    "cut:post-version": "pnpm run build",
    "lint": "eslint . --ext .ts,.cjs",
    "repodog": "repodog",
    "syncpack": "syncpack format && syncpack list-mismatches && syncpack lint-semver-ranges",
    "test": "node --require=suppress-experimental-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run syncpack && pnpm run build && pnpm run lint && pnpm run type-check && pnpm run test",
    "zcb:build": "NODE_OPTIONS=\"--loader ts-node/esm\" node ./bin/zcb.mjs build ./src/__testUtils__/configBuilder.ts ./src/__testUtils__/builtConfig.ts",
    "zcb:watch": "NODE_OPTIONS=\"--loader ts-node/esm\" node ./bin/zcb.mjs watch ./src/__testUtils__/configBuilder.ts ./src/__testUtils__/builtConfig.ts"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/json-schema": "^7.0.11",
    "@types/shelljs": "^0.8.12",
    "@types/yargs": "^15.0.15",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.7",
    "json-schema": "^0.4.0",
    "shelljs": "^0.8.5",
    "type-fest": "^3.10.0",
    "yargs": "^15.4.1",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.0"
  },
  "peerDependencies": {
    "@babel/runtime": "<8",
    "core-js": "<4",
    "lodash-es": "<5",
    "ts-node": "<11"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-decorators": "^7.23.3",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/prompt-cli": "^17.6.3",
    "@jest/globals": "^29.5.0",
    "@repodog/babel-config": "^1.1.11",
    "@repodog/cli": "^1.3.3",
    "@repodog/commitlint-config": "^1.1.6",
    "@repodog/eslint-config": "^1.1.19",
    "@repodog/eslint-config-jest": "^1.1.10",
    "@repodog/jest-config": "^1.1.11",
    "@repodog/markdownlint-config": "^1.1.5",
    "@repodog/prettier-config": "^1.1.5",
    "@repodog/rollup-config": "^1.1.8",
    "@repodog/syncpack-config": "^1.2.3",
    "@repodog/ts-config": "^1.1.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.16.7",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-jest": "^29.5.0",
    "babel-plugin-codegen": "^4.1.5",
    "babel-plugin-macros": "^3.1.0",
    "core-js": "^3.30.2",
    "cts-types": "^0.0.4",
    "del-cli": "^3.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-class-members": "^1.18.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unicorn": "^46.0.1",
    "fast-check": "^3.14.0",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lodash-es": "^4.17.21",
    "markdownlint-cli": "^0.34.0",
    "prettier": "^2.8.8",
    "rollup": "^3.21.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "suppress-experimental-warnings": "^1.1.17",
    "syncpack": "^12.0.0-alpha.0",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "config",
    "configuration",
    "zod"
  ]
}
